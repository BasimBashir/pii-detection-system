{
  "name": "pii-detection-chat-service",
  "version": "1.0.0",
  "description": "AI-powered PII detection service for chat applications using Google Gemini API",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
